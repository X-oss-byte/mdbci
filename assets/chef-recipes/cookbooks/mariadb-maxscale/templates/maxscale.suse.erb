[<%= node['maxscale']['repo_file_name'] %>]
name = <%= node['maxscale']['repo_file_name'] %>
enabled=1
autorefresh=0
baseurl=<%= node['maxscale']['repo'] %>/<%= node[:platform] %>/<%= node[:platform_version].to_i %>/$basearch
gpgkey=<%= node['maxscale']['repo_key'] %>/Maxscale-GPG-KEY.public
gpgcheck=0
